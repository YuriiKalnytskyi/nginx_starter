{
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "usePrettierrc": true }]
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "ignorePatterns": [
      "node_modules",
      "build",
      "dist",
      "public"
  ]
}
