{
  "name": "yobb",
  "version": "1.0.0",
  "description": "yobb",
  "type": "module",
  "author": "Yurii Kalnytskyi",
  "main": "server.js",
  "scripts": {
    "+++++++++++++++START_SCRIPT": "+++++++++++++++++++++++++++++++",
    "check_env_and_start": "env-cmd -f .env.${ENV} node ./check-env.js && env-cmd -f .env.${ENV} nodemon --experimental-specifier-resolution=node server.js",
    "start_server": "NODE_ENV=${NODE_ENV} npm run db:start_migrate_and_seed  && ENV=${ENV} npm run check_env_and_start",
    "start_script": "NODE_ENV=${NODE_ENV} ENV=${ENV} npm run start_server",
    "start:local": "NODE_ENV=local ENV=local npm run start_script",
    "start:local1": "NODE_ENV=local ENV=local npm run check_env_and_start",
    "start:dev": "NODE_ENV=dev ENV=dev npm run start_script",
    "+++++++++++++++DB_SCRIPT": "++++++++++++++++++++++++++++++++++",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "db:start_migrate_and_seed": "NODE_ENV=${NODE_ENV} npm run db:migrate && NODE_ENV=${NODE_ENV} npm run db:seed",
    "db:start:local": "NODE_ENV=local npm run db:start_migrate_and_seed",
    "db:fresh": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "+++++++++++++++FORMAT_SCRIPT": "++++++++++++++++++++++++++++++",
    "format:check": "prettier --check \"./**/*.{js,ts,jsx,tsx,css,html}\"",
    "format:fix": "prettier --write \"./**/*.{js,ts,jsx,tsx,css,html}\"",
    "lint:check": "eslint \"./**/*.js\""
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "datauri": "^4.1.0",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.6.0",
    "react": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.4.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "concurrently": "^5.0.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "nodemon": "^2.0.2",
    "prettier": "^2.5.1"
  }
}
